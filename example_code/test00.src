//Energema - Hello World!

effect print: bool -> bool;

handler defaultHandler() -> i32 {
    return(value: bool) { if value { 1 } else { 0 } },
    print(value: bool) { let res = continuation(!value); nativePrint(value); res }
}

fn main() {
    with defaultHandler() {
        let result = perform print(true);
        perform print(result)
    }
}